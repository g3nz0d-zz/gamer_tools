#This is a virtual d20 sided dice

endprogram=0
while endprogram != "q":
  import random
  print("Welcome to eDice")
  print("Press the Enter Key to Roll the D20")
  input()
  number=random.randint(0,20)
  if number==0:
      print("  /------\  )"
      print(" /        \)"
      print(" |   0    |)"
      print(" |        |)"
      print("  \------/)"
      print("OUCH!")
      Print("Type 'q' to Quit)"
      endprogram=input()
  if number==1:
      print("  /------\ )"
      print(" /        \)"
      print(" |   1    |)"
      print(" |        |)"
      print("  \------/ )"
